{
  "expo": {
    "name": "Temp-Mail.Lol",
    "slug": "temp-mail",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/temp-mail-icon-removebg.png",
    "scheme": "tempmail",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.tronics.tempmail",
      "notification": {
        "icon": "./assets/images/temp-mail-icon-removebg.png"
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/temp-mail-icon-removebg.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.tronics.tempmail",
      "permissions": [
        "INTERNET",
        "CAMERA",
        "WRITE_EXTERNAL_STORAGE",
        "READ_EXTERNAL_STORAGE"
      ],
      "versionCode": 1,
      "edgeToEdgeEnabled": true,
      "notification": {
        "icon": "./assets/images/temp-mail-icon-removebg.png",
        "color": "#0F5259"
      }
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/temp-mail-icon-removebg.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/temp-mail-icon-removebg.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "apiBaseUrl": "https://temp-mail-api.4zjyot.easypanel.host/",
      "router": {},
      "eas": {
        "projectId": "e942f9b0-6115-4ca4-be70-127d4a4fd766"
      }
    },
    "owner": "tronics",
    "privacy": "public",
    "githubUrl": "https://github.com/tempmail/tempmail",
    "description": "Generate temporary email addresses to protect your privacy. Receive emails without revealing your personal information.",
    "primaryColor": "#0F5259",
    "splash": {
      "image": "./assets/images/temp-mail-icon-removebg.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    }
  }
}
